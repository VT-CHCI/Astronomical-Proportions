<h1>Listing meeting_times</h1>

<table>
  <tr>
    <th>Monday</th>
    <th>Tuesday</th>
    <th>Wednesday</th>
    <th>Thursday</th>
    <th>Friday</th>
    <th>Saturday</th>
    <th>Sunday</th>
    <th>Start</th>
    <th>End</th>
    <th>Course</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @meeting_times.each do |meeting_time| %>
  <tr>
    <td><%= meeting_time.monday %></td>
    <td><%= meeting_time.tuesday %></td>
    <td><%= meeting_time.wednesday %></td>
    <td><%= meeting_time.thursday %></td>
    <td><%= meeting_time.friday %></td>
    <td><%= meeting_time.saturday %></td>
    <td><%= meeting_time.sunday %></td>
    <td><%= meeting_time.start %></td>
    <td><%= meeting_time.end %></td>
    <td><%= meeting_time.course %></td>
    <td><%= link_to 'Show', meeting_time %></td>
    <td><%= link_to 'Edit', edit_meeting_time_path(meeting_time) %></td>
    <td><%= link_to 'Destroy', meeting_time, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Meeting time', new_meeting_time_path %>
