 <div id="chooseObjects">
        <div id="leftList" class ="left">
          <div id="list1">
          </div>
        </div>

        <div id="rightList" class="right">
          <!-- <div id="list2" class="listContainer">
          </div> -->
        </div>
      </div>

      <div id="animateObjects" class="span10">
        <div>
          <div id="leftObject" class="objectAnimation left">
          </div>
          <div id="rightObject" class="objectAnimation right">
          </div>
        </div>
        <div id="description">
          <%= image_tag "arrows-double-reversed.svg", :id=>"reverseDescription",  :width=>"50" %>
          <p id="verboseDescription"></p>
        </div>
      </div>

      <div id="analogyObjects" class="span12">
        <div id="instructionText"  class="row"><p>
          The <span class="analogyDimension"></span> of <span class="orange" id="analogyleftObjectName1">
             </span><span class="orange" id="analogyleftObjectValue1"></span>
          is to the <span class="analogyDimension"></span> of <span class="maroon" id="analogyrightObjectName1"></span>
              <span class="maroon" id="analogyrightObjectValue1"></span></span> 
          <span class="right" id="analogyRatio1"></span>
          <br/><br/><br/>  </p>
        </div>
        <div class="row">
         <div id="analogyOriginalObjects" class="row">
          <div class="span6"> <p>
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            as the <span class="analogyDimension"></span> of <span class="orange" id="analogyleftObjectName2"></span> is </p>
            <div id="analogyleftObject" class="hidden" class="left"></div>
            <div id="analogySecondObjectsLeft" class="left"></div>
          </div>
          
          <div class="span6"> <p>
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 
            to the <span class="analogyDimension"></span> of <span class="maroon" id="analogyrightObjectName2"></span>. 
            <span class="right" id="analogyRatio2"></span>
            </p>
            <div id="analogyrightObject" class="hidden" class="right"></div>
            <div id="analogySecondObjectsRight" class="right"></div>
          </div>
        </div>
        
        <div id="analogySecondObjects" class="span10">
          <div>
            <div id="analogyAnimateleftObject" class="objectAnimation left"></div>
            <div id="analogyAnimaterightObject" class="objectAnimation right"></div>
          </div>

            

            
        </div>
        </div>
      </div>


      <div id="predictionObjects" class="span10">
        <div class="row">
          <p>
            <!-- If <span id="leftPredictionObjectName"></span> was this big, what size would <span id="rightPredictionObjectName"></span> be? <br /> -->
          Drag the smaller object to the correct size!
          <button type="button" id="predictionButton" class="btn btn-primary">Check me!</button>
        </p> 
          <span id="predictionRatio"><span id="predictionleftObjectSize"></span> : <span id="predictionrightObjectSize"></span></span>
          <span class="label label-info" id="predictionTooLarge">That's as close as I can get.</span>
          <span class="label label-success" id="predictionCorrect">Correct!</span>
          <span class="label label-warning" id="predictionCloseSmall">You're getting close. (Hint: Try making it smaller.)</span>
          <span class="label label-warning" id="predictionCloseLarge">You're getting close. (Hint: Try making it bigger.)</span>
          <span class="label label-important" id="predictionIncorrect">You're pretty far off.</span>
          <br /><br />
        </div>
        <div class="row">
          <div class="span6">
            <div id="predictionleftObject" class="left">
            </div>
          </div>
          
          

          <div class="span6">
            <div id="predictionrightObject" class="left">
            </div>
          </div>

          
        </div>
      </div>

